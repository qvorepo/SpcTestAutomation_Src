{
  "name": "spctestautomation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "node_modules\\.bin\\cypress run",
    "headTest": "npm run test -- --headed",
    "chromeTest": "npm run test -- --browser chrome --headless",
    "curdsideChannelTestSuite": "npm run test -- --reporter mochawesome  --browser chrome --headed --spec \"cypress/integration/examples/CurbsideChannelTest.\"",
    "routingDashboardTestSuite": "npm run test -- --record --key 3d9b3290-1c50-4f5e-ad10-f8d90b7a1db0 --browser electron --headed --spec \"cypress/integration/changeRoutingTemplates.feature,cypress/integration/CurbsideChannelTest.js,cypress/integration/API_SubmitCurbsideArrivalMessageTest.js,cypress/integration/SwitchCurbsideChannelStateTest.js,cypress/integration/LabelPrintTest.js\"",
    "GreenKartTest": "npm run test -- --spec \"cypress/integration/GreenKart/*\""
  },
  "author": "QuangVo",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^5.2.0",
    "cypress-cucumber-preprocessor": "^3.0.0",
    "mocha": "^8.1.3",
    "mochawesome": "^6.1.1",
    "sqlite3": "^5.0.0"
    
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
